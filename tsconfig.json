{
  "include": ["ts/*.ts"],
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "node16",
    "target": "es2022",
    "outDir": "js",

    "strict": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  }
}
